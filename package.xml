<?xml version="1.0"?>
<package>
  <name>lwr_safe_cartesian</name>
  <version>0.0.0</version>
  <description>Translate geometry_msgs/Pose messages (cartesian pose) to sensor_msgs/JointState messages (joint positions) for the KUKA LWR4+ with reachability and static collision checking</description>
  <maintainer email="andreas.bihlmaier@gmx.de">Andreas Bihlmaier</maintainer>
  <license>MIT</license>


  <!-- Url tags are optional, but mutiple are allowed, one per tag -->
  <!-- Optional attribute type can be: website, bugtracker, or repository -->
  <!-- Example: -->
  <!-- <url type="website">http://wiki.ros.org/lwr_safe_cartesian</url> -->


  <author email="andreas.bihlmaier@gmx.de">Andreas Bihlmaier</author>


  <!-- The *_depend tags are used to specify dependencies -->
  <!-- Dependencies can be catkin packages or system dependencies -->

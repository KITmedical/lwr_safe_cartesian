  <!-- Use build_depend for packages you need at compile time: -->
  <!--   <build_depend>message_generation</build_depend> -->
  <!-- Use buildtool_depend for build tool packages: -->
  <!--   <buildtool_depend>catkin</buildtool_depend> -->
  <!-- Use run_depend for packages you need at runtime: -->
  <!--   <run_depend>message_runtime</run_depend> -->
  <!-- Use test_depend for packages you need only for testing: -->
  <!--   <test_depend>gtest</test_depend> -->
  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>ahbstring</build_depend>
  <build_depend>lwr_analytical_kinematics</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>safe_cartesian_moveit</build_depend>
  <build_depend>general_purpose_interpolator</build_depend>
  <build_depend>safe_cartesian_msgs</build_depend>
  <run_depend>ahbstring</run_depend>
  <run_depend>lwr_analytical_kinematics</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>safe_cartesian_moveit</run_depend>
  <run_depend>general_purpose_interpolator</run_depend>
  <run_depend>safe_cartesian_msgs</run_depend>


